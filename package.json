{
  "name": "yt",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon dist/server/index.js",
    "watch": "tsc --watch --project tsconfig.server.json",
    "build:server": "tsc --project tsconfig.server.json",
    "build:next": "next build",
    "build": "npm run build:next && npm run build:server",
    "start": "NODE_ENV=production node dist/index.js",
    "git": "git push -u origin main",
    "next": "next",
    "tsc": "tsc"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "express": "^4.17.1",
    "next": "10.0.7",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "ytdl-core": "^4.4.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.28",
    "@types/react": "^17.0.2",
    "typescript": "^4.1.5"
  }
}
